<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <stroke android:color="#dadbdc" android:width="1dp" />
            <solid android:color="#b7b7b7"/>
            <corners android:radius="3dp" android:topLeftRadius="-3dp" android:bottomLeftRadius="-3dp"/>
            <size android:height="10dp" android:width="10dp" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip android:clipOrientation="horizontal" android:gravity="left">
            <shape android:shape="rectangle">
                <stroke android:color="#C24C47" android:width="1dp" />
                <solid android:color="#D83731"/>
                <corners android:radius="3dp" android:topRightRadius="-3dp" android:bottomRightRadius="-3dp"/>
                <size android:height="10dp" android:width="10dp" />
            </shape>
        </clip>
    </item>
</layer-list>